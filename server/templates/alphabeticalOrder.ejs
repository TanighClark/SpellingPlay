<%- include('header.ejs') %>
    <style>
        body {
            font-family: Helvetica, sans-serif;
            margin: 40px;
            font-size: 14pt;
        }

        h1 {
            font-size: 20pt;
            margin-bottom: 0;
        }

        p {
            margin-top: 0;
            margin-bottom: 30px;
        }

        ol.alpha-list {
            list-style-position: outside;
            padding-left: 20px;
        }

        .blank-line {
            border-bottom: 1px solid #000;
            height: 1.5em;
            width: 100%;
            margin-bottom: 15px;
        }
    </style>

    <ol class="alpha-list">
        <% for (let i=0; i < wordBank.length + 3; i++) { %>
            <li>
                <div class="blank-line"></div>
            </li>
            <% } %>
    </ol>